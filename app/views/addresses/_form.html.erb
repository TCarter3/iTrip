<%= form_with(model: [@location, @address], local: true, class: "ui form") do |f| %>
  <%= f.label :street %>
  <%= f.text_field :street %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <%= f.submit nil, class: "ui inverted purple button"  %>
<% end %>